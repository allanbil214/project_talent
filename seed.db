INSERT INTO `jobs` (`id`, `employer_id`, `title`, `description`, `job_type`, `location_type`, `location_address`, `salary_min`, `salary_max`, `salary_type`, `currency`, `experience_required`, `status`, `deadline`) VALUES
(1, 1, 'Senior Full Stack Developer', 'We are looking for an experienced Full Stack Developer to join our engineering team. You will work on building scalable web applications using modern technologies.\n\nResponsibilities:\n- Develop and maintain web applications\n- Write clean, maintainable code\n- Collaborate with cross-functional teams\n- Participate in code reviews\n\nRequirements:\n- 5+ years of experience\n- Strong knowledge of PHP/Laravel and React\n- Experience with MySQL and REST APIs\n- Excellent problem-solving skills', 'full-time', 'hybrid', 'Jakarta Pusat', 15000000, 25000000, 'monthly', 'IDR', 5, 'active', '2026-02-28'),

(2, 2, 'UI/UX Designer', 'Join our creative team as a UI/UX Designer. You will be responsible for creating beautiful and intuitive user interfaces for web and mobile applications.\n\nResponsibilities:\n- Design user interfaces and experiences\n- Create wireframes and prototypes\n- Conduct user research\n- Work closely with developers\n\nRequirements:\n- 3+ years of experience\n- Proficiency in Figma and Adobe XD\n- Strong portfolio\n- Understanding of design principles', 'freelance', 'remote', NULL, 10000000, 18000000, 'project', 'IDR', 3, 'active', '2026-02-15'),

(3, 3, 'Digital Marketing Specialist', 'We need a results-driven Digital Marketing Specialist to manage our clients campaigns and drive online growth.\n\nResponsibilities:\n- Develop and execute marketing strategies\n- Manage SEO and SEM campaigns\n- Create engaging content\n- Analyze campaign performance\n\nRequirements:\n- 4+ years of experience\n- Proven track record in SEO/SEM\n- Google Analytics certification\n- Excellent communication skills', 'full-time', 'onsite', 'Jakarta Selatan', 8000000, 15000000, 'monthly', 'IDR', 4, 'active', '2026-03-01'),

(4, 4, 'React Native Developer', 'Startup looking for a talented React Native Developer to build our mobile app from scratch. Great opportunity to work with cutting-edge technology.\n\nResponsibilities:\n- Build cross-platform mobile applications\n- Implement new features\n- Optimize app performance\n- Work in agile environment\n\nRequirements:\n- 3+ years with React Native\n- Experience with Redux\n- Published apps on App Store/Play Store\n- Strong JavaScript skills', 'contract', 'remote', NULL, 12000000, 20000000, 'monthly', 'IDR', 3, 'active', '2026-02-20'),

(5, 1, 'DevOps Engineer', 'TechCorp is seeking a DevOps Engineer to manage our cloud infrastructure and CI/CD pipelines.\n\nResponsibilities:\n- Manage AWS infrastructure\n- Implement CI/CD pipelines\n- Monitor system performance\n- Automate deployment processes\n\nRequirements:\n- 5+ years in DevOps\n- AWS certification preferred\n- Experience with Docker and Kubernetes\n- Strong scripting skills', 'full-time', 'hybrid', 'Jakarta Pusat', 18000000, 28000000, 'monthly', 'IDR', 5, 'active', '2026-03-15'),

(6, 2, 'Content Writer', 'Looking for a creative Content Writer to produce engaging content for our clients websites and social media.\n\nResponsibilities:\n- Write blog posts and articles\n- Create social media content\n- Research industry trends\n- Edit and proofread content\n\nRequirements:\n- 2+ years of experience\n- Excellent writing skills in English and Indonesian\n- SEO knowledge\n- Portfolio required', 'part-time', 'remote', NULL, 60000, 100000, 'hourly', 'IDR', 2, 'active', '2026-02-25'),

(7, 5, 'Data Scientist', 'Join our data team to build machine learning models that power our recommendation engine.\n\nResponsibilities:\n- Develop ML models\n- Analyze large datasets\n- Create data visualizations\n- Collaborate with engineering team\n\nRequirements:\n- Masters degree in related field\n- 5+ years of experience\n- Python and TensorFlow expertise\n- Strong statistical knowledge', 'full-time', 'hybrid', 'Jakarta Selatan', 20000000, 35000000, 'monthly', 'IDR', 5, 'pending_approval', '2026-03-10'),

(8, 3, 'Social Media Manager', 'We need a Social Media Manager to handle multiple client accounts and grow their online presence.\n\nResponsibilities:\n- Manage social media accounts\n- Create content calendars\n- Engage with audiences\n- Track and report metrics\n\nRequirements:\n- 3+ years of experience\n- Experience with all major platforms\n- Creative mindset\n- Analytics skills', 'full-time', 'onsite', 'Jakarta Selatan', 7000000, 12000000, 'monthly', 'IDR', 3, 'filled', '2026-01-31'),

(9, 4, 'Backend Developer (Node.js)', 'Looking for a Backend Developer to build scalable APIs for our growing platform.\n\nResponsibilities:\n- Design and build RESTful APIs\n- Optimize database queries\n- Write unit tests\n- Deploy to cloud infrastructure\n\nRequirements:\n- 4+ years with Node.js\n- Experience with Express/NestJS\n- MongoDB or PostgreSQL knowledge\n- Understanding of microservices', 'full-time', 'remote', NULL, 12000000, 22000000, 'monthly', 'IDR', 4, 'active', '2026-03-05'),

(10, 5, 'Frontend Developer (React)', 'E-Commerce Giant is hiring Frontend Developers to work on our customer-facing applications.\n\nResponsibilities:\n- Build responsive web interfaces\n- Implement designs pixel-perfect\n- Optimize performance\n- Write reusable components\n\nRequirements:\n- 3+ years with React\n- Experience with Redux/Context API\n- CSS expertise (Tailwind/SASS)\n- Attention to detail', 'full-time', 'hybrid', 'Jakarta Selatan', 10000000, 18000000, 'monthly', 'IDR', 3, 'active', '2026-02-28');

